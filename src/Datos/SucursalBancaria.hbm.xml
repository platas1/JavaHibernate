<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="Negocio.SucursalBancaria" table="sucursalbancaria" > <!--Hago referencia a la clase en Negocio (uso puntos)-->
        <id column="idSucursalBancaria" name="idSucursalBancaria" type="integer"/> <!--Clave primaria de la tabla en Heidi -->
        <property name="codigoSucursal" />
        <property name="nombre" />       
    </class>
  
</hibernate-mapping>
